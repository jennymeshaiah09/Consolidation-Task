# Google Gemini API Key for keyword generation
# Get your API key from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Google Ads API Credentials for MSV (Monthly Search Volume) lookup
# Follow the setup guide in GOOGLE_ADS_API_SETUP.md to get these credentials

# Developer Token (from Google Ads > Tools & Settings > API Center)
GOOGLE_ADS_DEVELOPER_TOKEN=your_developer_token_here

# OAuth Client ID (from Google Cloud Console > APIs & Services > Credentials)
GOOGLE_ADS_CLIENT_ID=your_client_id_here.apps.googleusercontent.com

# OAuth Client Secret (from Google Cloud Console > APIs & Services > Credentials)
GOOGLE_ADS_CLIENT_SECRET=your_client_secret_here

# Refresh Token (generated using generate_refresh_token.py script)
GOOGLE_ADS_REFRESH_TOKEN=your_refresh_token_here

# Customer ID (from Google Ads dashboard, 10 digits without dashes)
GOOGLE_ADS_CUSTOMER_ID=1234567890
